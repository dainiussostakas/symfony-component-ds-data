{
  "name": "ds/data",
  "type": "library",
  "description": "Data generators and converters",
  "version": "0.0.6",
  "require": {
    "php": ">=8.2"
  },
  "autoload": {
    "classmap": [
      "src"
    ],
    "psr-4": {
      "DS\\Data\\": ["src"],
      "DS\\Data\\Tests\\": ["tests"]
    },
    "exclude-from-classmap": ["tests"]
  },
  "require-dev": {
    "phpunit/phpunit": "^11",
    "phpstan/phpstan": "^1.10",
    "squizlabs/php_codesniffer": "3.7.2"
  }
}
