{
  "name": "ds/generator",
  "type": "library",
  "description": "Generator",
  "version": "0.0.3",
  "require": {
    "php": ">=8.2"
  },
  "autoload": {
    "classmap": [
      "src"
    ],
    "psr-4": {
      "DS\\Generator\\": ["src", "tests"]
    },
    "exclude-from-classmap": ["tests"]
  },
  "require-dev": {
    "phpunit/phpunit": "^11",
    "phpstan/phpstan": "^1.10",
    "squizlabs/php_codesniffer": "3.7.2"
  }
}
